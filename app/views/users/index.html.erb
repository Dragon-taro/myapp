<div class="innerWrapper">
  <h1>おすすめ師匠</h1>
  <div class="m_topUsers">
    <%= render "users_list", users: @top_users %>
  </div>
  <div class="showAndSearchZone">
    <div class="leftContent">
      <h2>最近の師匠</h2>
      <div class="m_normalUsers">
        <%= render "users_list", users: @users %>
      </div>
    </div>
    <div class="rightContent">
      <h2>検索</h2>
      <div id="SearchForm"></div>
    </div>
  </div>
</div>
<%= javascript_pack_tag 'SearchForm'%>
